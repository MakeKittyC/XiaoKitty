<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_fragment_vote">
        
    <fragment
        android:id="@+id/navigation_fragment_valo"
        android:name="app.ui.valo.ValoFragment"
        android:label="@string/title_fragment_valo" 
        tools:layout="@layout/fragment_valo" />

    <fragment
        android:id="@+id/navigation_fragment_task"
        android:name="app.ui.task.TaskFragment"
        android:label="@string/title_fragment_task"
        tools:layout="@layout/fragment_task" />

    <fragment
        android:id="@+id/navigation_fragment_vote"
        android:name="app.ui.vote.VoteFragment"
        android:label="@string/title_fragment_vote"
        tools:layout="@layout/fragment_vote" />

    <fragment
        android:id="@+id/navigation_fragment_just"
        android:name="app.ui.just.JustFragment"
        android:label="@string/title_fragment_just"
        tools:layout="@layout/fragment_just" />
        
    <fragment
        android:id="@+id/navigation_fragment_fast"
        android:name="app.ui.fast.FastFragment"
        android:label="@string/title_fragment_fast"
        tools:layout="@layout/fragment_fast" />
        
</navigation>